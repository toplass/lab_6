x = "ААААААООООФФФФФФФФ"
a =['А', 'О', 'У', 'Ы', 'Э', 'И', 'Е', 'Ё', 'Ю', 'Я']
for i in range(1, len(x)+1):
    n = [str(t) for t in x if t in a]
print(len(n))
##
def f (x):
    c_m = str(x).count('*') + 1
    p_x = x.split('*')
    p = 1
    for i in p_x:
        p *= int(i)
###
def f(x):
    t = 0
    for i in x[1::2]:
        if t <= i:
            t = i
    print(t)
###
a = [1,2,3,4,5,15,7,8,12,10]
c = 0
for i in range(0, len(a)):
    if a[1] < a[i] < a[9]:
        c = i + 1
print(c)
